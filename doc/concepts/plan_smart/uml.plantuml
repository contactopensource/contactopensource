@startuml uml
skinparam monochrome true
skinparam linetype ortho
hide circle

entity plan_smart {
  specific_id : plan_item(id)+
  measurable_id : plan_item(id)+
  actionable_id : plan_item(id)+
  relevant_id : plan_item(id)+
  timely_id : plan_item(id)+
}

entity plan_item {
  name : string+
  summary : string
  description : text
  disambiguation : text
  unit_id : unit(id)+
  value : value
}

@enduml
