@startuml uml
skinparam monochrome true
skinparam linetype ortho
hide circle

entity currency_type {
  .. meta ..
  id : uuid ⚡
  lock_version : integer
  created_at : timestamp_utc_usec ⚡
  created_by : url ⚡
  updated_at : timestamp_utc_usec ⚡
  updated_by : url ⚡
  retired_at : timestamp_utc_usec ⚡
  retired_by : url ⚡
  locale_code : string ⚡
  tagging : string ⚡
  .. Main ..
  name : text ⚡
  code : text ⚡
  symbol : text ⚡
}

@enduml
