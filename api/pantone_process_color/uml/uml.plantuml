@startuml uml
skinparam monochrome true
skinparam linetype ortho
hide circle

entity pantone_process_color {
  .. Meta ..
  id : uuid ⚡
  sign : char ⚡
  lock_version : integer
  created_at : timestamp_utc_usec ⚡
  created_by : url ⚡
  updated_at : timestamp_utc_usec ⚡
  updated_by : url ⚡
  deleted_at : timestamp_utc_usec ⚡
  deleted_by : url ⚡
  locale_code : string ⚡
  .. Main ..
  key : string ⚡
  name : string ⚡
  code : string(3) ⚡
  stock : string(1) ⚡
}

@enduml
